{
  "name": "stargazer-wallet-ext",
  "version": "2.1.1",
  "description": "Cross-chain compatible wallet for Constellation and Ethereum",
  "private": true,
  "repository": "https://github.com/StardustCollective/stargazer-wallet-ext.git",
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0",
    "yarn": ">= 1.0.0"
  },
  "packageManager": "yarn@1.19.0",
  "scripts": {
    "dev:chrome": "yarn workspace extension dev:chrome",
    "dev:firefox": "yarn workspace extension dev:firefox",
    "build:chrome": "yarn workspace extension build:chrome",
    "build:chrome:prod": "yarn workspace extension build:chrome:prod",
    "build:firefox": "yarn workspace extension build:firefox",
    "build": "yarn workspace extension build",
    "test:e2e:chrome": "yarn workspace extension test:e2e:chrome",
    "test:e2e:chrome:silent": "yarn workspace extension test:e2e:chrome:silent",
    "analyze:chrome": "yarn workspace extension analyze:chrome",
    "android": "yarn workspace native run-android",
    "ios": "yarn workspace native run-ios",
    "start": "yarn workspace native start",
    "lint": "npx eslint --ext=.js,.jsx .",
    "lint:fix": "npx eslint --ext=.js,.jsx . --fix",
    "clean": "npx rimraf ./node_modules && cd source/native && rm -rf ./node_modules && cd .. && cd web && rm -rf ./node_modules",
    "clean:pods": "cd source/native/ios && rm -rf Pods",
    "clean:all": "yarn clean && yarn clean:pods",
    "install:pods": "cd source/native/ios && pod install",
    "postinstall": "npx patch-package"
  },
